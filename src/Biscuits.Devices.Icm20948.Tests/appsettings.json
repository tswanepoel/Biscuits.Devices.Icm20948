{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:l}{Exception}{NewLine}"
        }
      }
    ]
  },
  "Icm20948Options": {
    "BusId": 0,
    "ChipSelectLine": 0,
    "InterruptPinNumber": 25
  }
}
